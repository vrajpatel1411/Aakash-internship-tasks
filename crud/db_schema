create table users (user_id Varchar(10) not null,user_name Varchar(50) not null,mobile_number varchar(10) not null,email varchar(50) not null,address varchar(100) not null, primary key(user_id));

create table banking1 (sr_no int(10) auto_increment,user_id varchar(10) not null,user_name Varchar(50) not null,mobile_number varchar(12) not null unique,login_id varchar(10) unique,password varchar(10)  unique,primary key(sr_no));

create table balance_details(user_id Varchar(10) not null unique,login_id Varchar(10) not null ,balance int(10) not null)

